{
  "version": "1.5.1",
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "outDir": "build",
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "strict": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
    "include": [
        "src"
    ]
}